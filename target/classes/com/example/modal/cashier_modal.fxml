<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.text.Font?>

<HBox xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.components.Modal.CashierModalController">
    <VBox HBox.hgrow="ALWAYS" styleClass="p-2" spacing="25">
        <VBox spacing="5">
            <Label text="Jumlah modal hari ini" styleClass="fs-4" />
            <TextField styleClass="form-control" maxWidth="1.7976931348623157E308" fx:id="modal" promptText="Masukkan modal hari ini" />
        </VBox>
        
        <HBox alignment="CENTER_RIGHT" spacing="5">
            <Button HBox.hgrow="ALWAYS" fx:id="close_button" styleClass="btn, bg-danger" text="Tutup" onAction="#close" />
            <Button HBox.hgrow="ALWAYS" fx:id="action_button" styleClass="btn, bg-primary" text="Simpan" onAction="#openKasir" />
        </HBox>
    </VBox>
</HBox>